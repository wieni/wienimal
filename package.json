{
  "name": "wienimal",
  "version": "2.0.0",
  "description": "Wienimal, a nice admin experience for Drupal 8.",
  "main": "",
  "scripts": {
    "build-scss": "node-sass --include-path scss --output-style compressed --output css scss",
    "build-postcss": "postcss -r css/*",
    "copy-select2": "cp -R node_modules/select2/dist vendor/select2",
    "build": "yarn run build-scss && yarn run build-postcss & yarn run copy-select2"

  },
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "babel-core": "^6.8.0",
    "babel-preset-es2015": "^6.6.0",
    "cssnano": "^3.10.0",
    "node-sass": "^4.5.1",
    "postcss-cli": "^3.0.0",
    "postcss-load-config": "^1.2.0",
    "yargs": "^5.0.0"
  },
  "postcss": {
    "plugins": {
      "autoprefixer": {
        "browsers": [
          "last 2 versions",
          "IE 10",
          "IE 11",
          "Firefox >= 20"
        ],
        "add": true
      },
      "cssnano": {}
    }
  },
  "dependencies": {
    "select2": "^4.0.3"
  }
}
